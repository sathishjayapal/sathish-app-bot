{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "size": "Medium",
      "weight": "Bolder",
      "text": "Spring App Development"
    },
    {
      "type": "TextBlock",
      "text": "Spring application development. I can consult or build out a mini project for you to kick start your idea.",
      "wrap": true
    },
    {
      "type": "FactSet",
      "facts": [
        {
          "title": "Like Count:",
          "value": "${likeCount}"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Action.Execute",
      "title": "I Like This!",
      "verb": "userlike",
      "fallback": "Action.Submit"
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4"
}
